import java.util.Scanner;

public class Factorial {

	private static Scanner in;

	public static void main(String[] args) {
		in = new Scanner(System.in);
		System.out.println("Введите число");
		int n = in.nextInt();
		int result = 1;
		for (int i = 2; i <= n; i++)
			result *= i;
		System.out.println(result);
		if (n == 0 || n == 1)
			System.out.println(result);
	}
}
